        <Image  x:Name="image" HorizontalAlignment="Left" Height="100" Margin="175,37,0,0" VerticalAlignment="Top" Width="100"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="143,147,0,0" TextWrapping="Wrap" Text="Editor Mode is on." VerticalAlignment="Top" FontSize="20" FontFamily="Kano regular"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="68,179,0,0" TextWrapping="Wrap" Text="Most things will be off, but the minimal features will still be on." VerticalAlignment="Top" Width="334"/>

